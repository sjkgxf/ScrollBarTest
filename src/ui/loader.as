package ui
{
	import flash.display.Loader;
	
	public class loader extends Loader
	{
		private var Name:String;
		public function loader()
		{
			super();
		}
		
		public function get Names():String
		{
			return Name;
		}
		
		public function set Names(myname:String):void
		{
			Name = myname;
		}
	}
}